	Bdsk-Url-2 = {http://arxiv.org/abs/1709.04954},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2017ApJ...850..180J}}
@article{Fujimoto:2018aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2018ApJ...861....7F},
