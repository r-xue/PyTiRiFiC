	Author = {{Stark}, D.~P. and {Swinbank}, A.~M. and {Ellis}, R.~S. and {Dye}, S. and {Smail}, I.~R. and {Richard}, J.},
	Date-Added = {2018-09-17 14:02:32 -0500},
