	Arxivurl = {http://arxiv.org/abs/1802.02136},
	Author = {{Fujimoto}, S. and {Ouchi}, M. and {Kohno}, K. and {Yamaguchi}, Y. and {Hatsukade}, B. and {Ueda}, Y. and {Shibuya}, T. and {Inoue}, S. and {Oogi}, T. and {Toft}, S. and {G{\'o}mez-Guijarro}, C. and {Wang}, T. and {Espada}, D. and {Nagao}, T. and {Tanaka}, I. and {Ao}, Y. and {Umehata}, H. and {Taniguchi}, Y. and {Nakanishi}, K. and {Rujopakarn}, W. and {Ivison}, R.~J. and {Wang}, W.-h. and {Lee}, M.~M. and {Tadaki}, K.-i. and {Tamura}, Y. and {Dunlop}, J.~S.},
