	Author = {{Sliwa}, K. and {Wilson}, C.~D. and {Petitpas}, G.~R. and {Armus}, L. and {Juvela}, M. and {Matsushita}, S. and {Peck}, A.~B. and {Yun}, M.~S.},
	Date-Added = {2018-09-18 10:47:15 -0500},
