	File = {{B1C9F925-40DB-4160-94A3-CED6F0819679.pdf:/Users/Rui/Dropbox/Library.papers3/Files/B1/B1C9F925-40DB-4160-94A3-CED6F0819679.pdf:application/pdf;B1C9F925-40DB-4160-94A3-CED6F0819679.pdf:/Users/Rui/Dropbox/Library.papers3/Files/B1/B1C9F925-40DB-4160-94A3-CED6F0819679.pdf:application/pdf}},
	Journal = {Monthly Notices of the Royal Astronomical Society},
