	Author = {{de Blok}, W.~J.~G. and {Walter}, F. and {Smith}, J.-D.~T. and {Herrera-Camus}, R. and {Bolatto}, A.~D. and {Requena-Torres}, M.~A. and {Crocker}, A.~F. and {Croxall}, K.~V. and {Kennicutt}, R.~C. and {Koda}, J. and {Armus}, L. and {Boquien}, M. and {Dale}, D. and {Kreckel}, K. and {Meidt}, S.},
	Date-Added = {2019-04-19 16:22:54 -0500},
