	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2018MNRAS.tmpL.202K}}
@article{Obreschkow:2013aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2013ApJ...777..140O},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1309.6051},
	Author = {{Obreschkow}, D. and {Meyer}, M.},
	Date-Added = {2018-12-01 18:16:13 -0600},
