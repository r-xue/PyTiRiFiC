	Bdsk-Url-2 = {http://arxiv.org/abs/1312.3320},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2014A%26A...561A.122L}}
@article{Pavesi:2018aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2018ApJ...864...49P},
