	Journal = {\aj},
	Keywords = {dark matter, galaxies: dwarf, galaxies: fundamental parameters, galaxies: kinematics and dynamics, galaxies: spiral, galaxies: structure},
	Language = {English},
	Local-Url = {file://localhost/Users/Rui/Dropbox/Library.papers3/Files/81/81A2120D-D6B0-4D1E-9B2F-7C223FD1220A.pdf},
