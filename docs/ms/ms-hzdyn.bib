	Author = {{Hearin}, A.~P. and {Campbell}, D. and {Tollerud}, E. and {Behroozi}, P. and {Diemer}, B. and {Goldbaum}, N.~J. and {Jennings}, E. and {Leauthaud}, A. and {Mao}, Y.-Y. and {More}, S. and {Parejko}, J. and {Sinha}, M. and {Sip{\"o}cz}, B. and {Zentner}, A.},
	Date-Added = {2018-11-21 12:51:47 -0600},
