	Author = {{Erroz-Ferrer}, S. and {Knapen}, J.~H. and {Leaman}, R. and {D{\'{\i}}az-Garc{\'{\i}}a}, S. and {Salo}, H. and {Laurikainen}, E. and {Querejeta}, M. and {Mu{\~n}oz-Mateos}, J.~C. and {Athanassoula}, E. and {Bosma}, A. and {Comer{\'o}n}, S. and {Elmegreen}, B.~G. and {Mart{\'{\i}}nez-Valpuesta}, I.},
	Date-Added = {2018-11-06 15:29:21 -0600},
