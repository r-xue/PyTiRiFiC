	Arxivurl = {http://arxiv.org/abs/1810.10558},
	Author = {{Wang}, T. and {Elbaz}, D. and {Daddi}, E. and {Liu}, D. and {Kodama}, T. and {Tanaka}, I. and {Schreiber}, C. and {Zanella}, A. and {Valentino}, F. and {Sargent}, M. and {Kohno}, K. and {Xiao}, M. and {Pannella}, M. and {Ciesla}, L. and {Gobat}, R. and {Koyama}, Y.},
	Date-Added = {2018-12-02 00:23:47 -0600},
